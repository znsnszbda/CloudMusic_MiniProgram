/* pages/login/login.wxss */.wrapper{
  position:relative;
  z-index: 90;
  padding-bottom: 40rpx;
}

.left-top-sign{
  font-size: 120rpx;
  color: #777777;
  position:relative;
  left: -16rpx;
  opacity: .5;
}

.welcome{
  position:relative;
  left: 50rpx;
  top: -90rpx;
  font-size: 46rpx;
  color: #f5f5f5;
}


.input-content{
  padding: 0 60rpx;
}
.input-item{
  display:flex;
  align-items: center;
  padding-left: 30rpx;
  background:#f8f6fc;
  height: 80rpx;
  border-radius: 4px;
  margin-bottom: 50rpx;
  overflow: hidden;
}

.input-item:last-child{
  margin-bottom: 0;
}
.input-item .get-captcha{
  background-color: #8a1313;
  color: #fff;
  font-size: 28rpx;
  width: 250rpx;
  height: 100%;
  line-height: 80rpx;
  padding: 0 10rpx;
  border-radius: 0%;
}

.input-item input{
  height: 60rpx;
  font-size: 30rpx;
  color: #333333;
  width: 100%;
}
.confirm-btn{
  width: 630rpx!important;
  height: 76rpx;
  line-height: 76rpx;
  border-radius: 50rpx;
  margin-top: 70rpx;
  background: #8a1313;
  color: #fff;
  font-size: 32rpx;
  padding: 0;
}
.confirm-btn2:after{
  border-radius: 100px;
}

.toPassword{
  width: 200rpx;
  height: 200rpx;
  line-height: 200rpx;
  font-size: 32rpx;
  color: #4399fc;
  text-align: center;
  margin: 0 auto;
}

.register-section{
  position:absolute;
  left: 0;
  bottom: 50rpx;
  width: 100%;
  font-size: 28rpx;
  color: #606266;
  text-align: center;

}
.register-section text{
  color: #4399fc;
  margin-left: 10rpx;
}
